{
  "name": "Page Notes",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "Lets you take notes for web pages and sync them automatically to your Google Docs.",
  "background": {
     "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Page Notes",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "https://www.googleapis.com/",
    "https://accounts.google.com/o/oauth2/token"
   ]
}
