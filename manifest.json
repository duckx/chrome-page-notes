{
  "name": "Page Notes",
  "version": "2.2.4",
  "manifest_version": 2,
  "description": "Take notes for web pages and access them with a single click whenever you visit those web pages again. (Syncs to Google Drive)",
  "background": {
     "scripts": ["gfile.js", "pagenotes.js", "oauth2.js", "background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Page Notes",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "https://www.googleapis.com/",
    "https://accounts.google.com/"
   ]
}
