{
  "name": "Page Notes",
  "version": "2.1.3",
  "manifest_version": 2,
  "description": "Take page specific notes, notes that will be available to you whenever you visit those web pages. Sync your notes to your Google Drive for multi-device setup and more.",
  "background": {
     "scripts": ["gfile.js", "pagenotes.js", "oauth2.js", "background.js"]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Page Notes",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "https://www.googleapis.com/",
    "https://accounts.google.com/"
   ]
}
